{
  "name": "side-effect-js",
  "version": "2.1.0",
  "description": "New way to test application in real browser without the need of mock API, emulate any desired state in seconds.",
  "main": "dist/side-effect-js.min.js",
  "types": "dist/side-effect-js.d.ts",
  "scripts": {
    "test": "cd ./test-directory && npm i && npm test",
    "build": "tsc",
    "minify": "uglifyjs --compress --mangle --output dist/side-effect-js.min.js -- dist/side-effect-js.js",
    "prepublishOnly": "tsc && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladbash/side-effect-emulator.git"
  },
  "keywords": [
    "side effects",
    "effects",
    "mock api",
    "Single page application testing",
    "spa-testing",
    "react-testing",
    "angular-testing",
    "vue-testing"
  ],
  "author": "Elad Ben Shmuel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eladbash/side-effect-emulator/issues"
  },
  "homepage": "https://github.com/eladbash/side-effect-emulator#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.7.17",
    "uglify-js": "^3.8.1"
  }
}
